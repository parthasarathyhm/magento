<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/custom1/mymodule2">
        <service class="Custom1\Mymodule2\Api\DemoInterface" method="getProfile"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/custom1/mylist">
        <service class="Custom1\Mymodule2\Api\DemoInterface" method="setCustomList"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>